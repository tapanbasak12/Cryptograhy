#include <iostream>
using namespace std;
 
int gcd(int a, int b)
{
    if (a == 0)
        return b;
    return gcd(b % a, a);
}
 
// Finding Phi(n) = Number of co-primes of n
int phi(unsigned int n)
{
    unsigned int result = 1;
    for (int i = 2; i < n; i++)
        if (gcd(i, n) == 1)
            result++;
    return result;
}
 
int main()
{
    int n=10002;
    
    cout << "phi("<<n<<") = " << phi(n) << endl;
    return 0;
}
